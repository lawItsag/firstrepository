<apex:page Controller="AaController" >
<apex:form >
	<apex:pageBlock >
		<apex:pageBlockSection >
				
				<apex:pageBlockTable value="{!details}" var="cd" >
					<apex:column value="{!cd.Quantity}"/>
					<apex:column value="{!cd.PricebookEntryId}"/>
					<apex:column value="{!cd.OpportunityId}"/>
				</apex:pageBlockTable>
		</apex:pageBlockSection>
	</apex:pageBlock>
	</apex:form>
</apex:page>